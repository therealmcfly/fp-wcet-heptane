<?xml version="1.0"?>
<!DOCTYPE CONFIGURATION SYSTEM "config.dtd"> 

<CONFIGURATION>

<TARGET NAME="RISCV" ENDIANNESS="LITTLE"/>

<!-- Compiler toolchain -->
<!-- ****************** -->

<!-- Preprocessor -->
<PREPROCESSOR NAME="/home/eugene/heptane-master/CROSS_COMPILERS/RISCV/bin/riscv64-unknown-elf-cpp" OPT="-DRISCV_ARCHI -I. -I.."/>
<!-- Compiler (options -c -S are automatically put to generate assembly code) -->
<!-- Option -ggdb should be used if html output of critical path is asked for in subsequent WCET estimation -->
<COMPILER NAME="/home/eugene/heptane-master/CROSS_COMPILERS/RISCV/bin/riscv64-unknown-elf-gcc" OPT="-O0 -fno-builtin -march=rv32i -mabi=ilp32"/>
<!-- Assembler -->
<ASSEMBLER NAME="/home/eugene/heptane-master/CROSS_COMPILERS/RISCV/bin/riscv64-unknown-elf-as" OPT=""/>
<!-- Linker (option -T script is automatically set when SCRIPT is specified) -->
<LINKER NAME="/home/eugene/heptane-master/CROSS_COMPILERS/RISCV/bin/riscv64-unknown-elf-ld" OPT="-m elf32lriscv" SCRIPT=""/>
<!-- Objdump -->
<OBJDUMP NAME="/home/eugene/heptane-master/CROSS_COMPILERS/RISCV/bin/riscv64-unknown-elf-objdump" OPT=" -ztd "/>
<!-- Readelf (called with option -S) -->
<READELF NAME="/home/eugene/heptane-master/CROSS_COMPILERS/RISCV/bin/riscv64-unknown-elf-readelf" OPT="-W "/>


<!-- Directories of inputs, temporaries and outputs (default values . /tmp and .) -->
<!-- **************************************************************************** -->
<INPUTDIR NAME="."/>
<TMPDIR NAME="/tmp"/>
<RESULTDIR NAME="results"/>

<!-- Program parameters -->
<!-- ****************** -->

<!-- NAME: program name, used as a file name prefix when generating outputs in RESULTDIR -->
<!-- SOURCEFILE NAME: source file name (merged with INPUTDIR to load file) -->
<!-- SOURCEFILE TYPE: C/ASM/BINARY -->
<!-- ANNOTATIONFILE: input file defining the loop bounds, required for BINARY or ASM files only. Example: ANNOTATIONFILE="annot.xml" ).-->
<!-- Several C files are accepted, but not for BINARY/ASM types. -->

<PROGRAM NAME = "test2" ENTRYPOINT = "main" >
  <SOURCEFILE NAME="test2.c" TYPE="C" />
</PROGRAM>

<!-- OTHER OPTIONS -->
<STATISTICS VALUE="NO"/>

</CONFIGURATION>
